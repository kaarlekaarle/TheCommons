{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p sync:features:watch vite:dev",
    "vite:dev": "vite",
    "build": "npm run sync:features && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sync:features": "tsx ../scripts/sync_features.ts",
    "sync:features:watch": "chokidar 'src/config/features.ts' -c 'npm run sync:features'",
    "check:features": "git diff --exit-code src/config/features.generated.json || (echo 'âœ– features.generated.json is stale. Run: npm run sync:features' && exit 1)"
  },
  "dependencies": {
    "@sentry/react": "^10.4.0",
    "@sentry/tracing": "^7.120.4",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
